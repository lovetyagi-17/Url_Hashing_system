    "paths": {
        "/api/v1/admin/signup": {
            "post": {
                "tags": [
                    "admin"
                ],
                "summary": "API to add User Account",
                "description": "API to add User Account",
                "operationId": "adminvalidateSignup",
                "responses": {
                    "200": {
                        "description": "Success Response"
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "pattern": "^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$",
                                        "description": "Email of User"
                                    },
                                    "password": {
                                        "type": "string",
                                        "description": "Password of User Account"
                                    }
                                },
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "additionalProperties": false
                            },
                            "example": {
                                "email": "gs593513@gmail.com",
                                "password": "234567890"
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/admin/login": {
            "post": {
                "tags": [
                    "admin"
                ],
                "summary": "API to Login",
                "description": "API to Login",
