                                    "email": {
                                        "type": "string",
                                        "pattern": "^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$",
                                        "description": "Email of User"
                                    },
                                    "password": {
                                        "type": "string",
                                        "description": "Password of Admin Account"
                                    }
                                },
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "additionalProperties": false
                            },
                            "example": {
                                "email": "gs593513@gmail.com",
                                "password": "234567890"
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/admin/utm": {
            "post": {
                "tags": [
                    "admin"
                ],
                "summary": "API to Create shortUrl",
                "description": "API to Create shortUrl",
                "operationId": "adminvalidateCreateUrl",
                "responses": {
                    "200": {
                        "description": "Success Response"
                    }
                },
                "security": [
                    {
                        "adminAuthorization": []
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "url": {
                                        "type": "string",
                                        "description": "url"
                                    }
                                },
                                "required": [
                                    "url"
                                ],
                                "additionalProperties": false
                            },
                            "example": {
                                "url": "https://google.com"
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/admin/utms": {
            "get": {
                "tags": [
                    "admin"
                ],
                "responses": {
                    "200": {
                        "description": "Success Response"
                    }
                },
                "security": [
                    {
                        "adminAuthorization": []
                    }
                ]
            }
        },
